{
  "configurations": [
    {
      "name": "storybook",
      "outputCapture": "std",
      "request": "launch",
      "runtimeArgs": ["exec", "--", "storybook", "dev", "--host", "0.0.0.0"],
      "runtimeExecutable": "npm",
      "serverReadyAction": {
        "action": "openExternally",
        "killOnServerStop": true,
        "pattern": "Local:\\s+(\\S+)",
        "uriFormat": "%s"
      },
      "type": "node"
    }
  ],
  "version": "0.2.0"
}
